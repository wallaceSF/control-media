<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\MediaPersonLoan" table="media_person_loan">
    <indexes>
      <index name="media_person_loan_media_id_fk" columns="media"/>
      <index name="media_person_loan_person_id_fk" columns="person"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="media" target-entity="App\Entity\Media" fetch="EAGER">
      <join-columns>
        <join-column name="media" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="person" target-entity="App\Entity\Person" fetch="EAGER">
      <join-columns>
        <join-column name="person" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <field name="returnedAt" type="date" column="returned_at" nullable="true"/>
  </entity>
</doctrine-mapping>
